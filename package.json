{
  "name": "html5-tetris-clone",
  "version": "1.0.0",
  "description": "A simple Tetris game clone written in HTML, JavaScript and CSS",
  "main": "index.html",
  "author": "Ioannis Tziligkakis <gtsiligkakis@gmail.com>",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html --public-url ./ --out-dir docs",
    "prettify": "prettier --write ./src/**/*.js"
  },
  "dependencies": {
    "parcel-bundler": "^1.6.1",
    "parcel-plugin-static-files-copy": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.2.1"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [],
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  }
}
